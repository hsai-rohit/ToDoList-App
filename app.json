{
  "expo": {
    "name": "To Do List App",
    "slug": "todolist-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png", 
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png", 
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png", 
        "backgroundColor": "#ffffff"
      },
      "package": "com.sairohit.todolistapp"
      
    },
    "web": {
      "favicon": "./assets/favicon.png" 
    },
    "experiments": {
      "reactCanary": true
    }
  }
}